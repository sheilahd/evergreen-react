mControl\n              className=\"form-control\"\n              name=\"email\"\n              value={email}\n              onChange={this.onChange}\n              type=\"email\"\n              placeholder=\"Email Address\"\n              required\n              autofocus\n            />\n          </FormGroup>\n\n          <FormGroup id=\"inputPassword\">\n            <FormControl\n              className=\"form-control\"\n              name=\"password\"\n              value={password}\n              onChange={this.onChange}\n              type=\"password\"\n              placeholder=\"Password\"\n              required\n            />\n          </FormGroup>\n\n          <button\n            className=\"btn btn-lg  btn-block btn-login text-uppercase font-weight-bold mb-2\"\n            disabled={isInvalid}\n            type=\"submit\"\n          >\n            Sign In\n          </button>\n\n          {error && <p>{error.message}</p>}\n        </form>\n      </div>\n    );\n  }\n}\n\nconst SignInForm = compose(withRouter, withFirebase)(SignInFormBase);\n\nexport default SignInPage;\n\nexport { SignInForm };\n","/Users/sheilahernandezdiaz/HOA/evergreen-react/src/components/Landing/header.js",["21"],"/Users/sheilahernandezdiaz/HOA/evergreen-react/src/components/Landing/about.js",["22","23"],{"ruleId":"24","severity":1,"message":"25","line":6,"column":10,"nodeType":"26","messageId":"27","endLine":6,"endColumn":13},{"ruleId":"24","severity":1,"message":"25","line":9,"column":10,"nodeType":"26","messageId":"27","endLine":9,"endColumn":13},{"ruleId":"24","severity":1,"message":"28","line":5,"column":9,"nodeType":"26","messageId":"27","endLine":5,"endColumn":16},{"ruleId":"24","severity":1,"message":"29","line":5,"column":8,"nodeType":"26","messageId":"27","endLine":5,"endColumn":13},{"ruleId":"24","severity":1,"message":"30","line":8,"column":10,"nodeType":"26","messageId":"27","endLine":8,"endColumn":14},"no-unused-vars","'Row' is defined but never used.","Identifier"