s;\n            this.setState({\n              users: usersList,\n            });\n          });\n      });\n\n      this.setState({\n        users: usersList,\n        loading: false,\n      });\n    });\n  }\n\n  componentWillUnmount() {\n    this.props.firebase.users().off();\n  }\n\n  render() {\n    const { users, loading } = this.state;\n\n    return (\n      <div\n        style={{\n          position: \"center\",\n          width: \"80%\",\n          marginLeft: \"10%\",\n          marginRight: \"10%\",\n          marginTop: \"5%\",\n        }}\n      >\n        {loading && <div>Loading ...</div>}\n\n        {users.map((user) => (\n          <ul>\n            <span>\n              Tester Name: <strong>{user.username}</strong>\n            </span>\n            <br />\n            <span>\n              Tester E-Mail:<strong>{user.email}</strong>\n            </span>\n            <br />\n            <span>\n              {/* <Link\n                to={{\n                  pathname: `${ROUTES.ADMIN}/${user.uid}`,\n                  state: { user },\n                }}\n              >\n                Details\n              </Link> */}\n            </span>\n\n            <div className=\"table-responsive-md\">\n              <Table\n                responsive=\"sm\"\n                className=\"table-bordered table-secondary \"\n              >\n                <thead className=\"table-dark text-center\">\n                  <tr>\n                    <th> Request Title </th>\n                    <th> Category</th>\n                    <th> Description</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  {user.addins &&\n                    user.addins.map((addin) => {\n                      return (\n                        <tr>\n                          <td> {addin.title} </td>\n                          <td>{addin.category}</td>\n                          <td>{addin.description}</td>\n                        </tr>\